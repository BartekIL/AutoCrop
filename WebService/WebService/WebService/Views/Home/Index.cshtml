@{
    ViewBag.Title = "Index";
}
<script src="/Scripts/jquery-1.8.2.js"></script>
<script src="/Scripts/ajaxfileupload.js"></script>
<div class="row">
    <div class="col-md-4">
        <a href="#">
            <img alt="" src="Content/img/Bez-nazwy-1.png" />
        </a>
    </div>
    <div class="col-md-4">
        <a href="#">
            <img alt="" src="Content/img/Obrazek.jpg" />
        </a>
    </div>
    <div class="col-md-4">
        <a href="#">
            <img alt="" src="Content/img/Obrazek2.jpg" />
        </a>
    </div>
    <div class="col-md-4">
        <a href="#">
            <img alt="" src="Content/img/Obrazek3.JPG" />
        </a>
    </div>
    <div class="col-md-4">
        <a href="#">
            <img alt="" src="Content/img/Obrazek4.jpg" />
        </a>
    </div>
    <div class="col-md-4">
        <a href="#">
            <img alt="" src="Content/img/Obrazek5.jpg" />
        </a>
    </div>
</div>

<script type="text/javascript">
    var image = '';
    $(document).ready(function () {
        $('.row a').click(function () {
            image = $('img', $(this)).attr('src');
            SendImage();
            return false;
        });
    });
    function SendImage() {
        $.ajax({
            method: 'POST',
            url: '/home/index',
            data: { "Image": image, },
            success: function (data, status) {
                alert(data);
            },
            error: function (data, status, e) {
                alert("error");
            },
        });
    }
</script>
